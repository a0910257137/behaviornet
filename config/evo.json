{
    "train_batch_size": 2,
    "test_batch_size": 2,
    "epochs": 120,
    "learn_rate": 0.001,
    "model_path": "/work/anders1234/WF/archive_model/evo_lateral",
    "summary": {
        "scalar_freq": 50,
        "hist_freq": 150,
        "img_freq": 250,
        "log_dir": "/work/anders1234/WF/archive_model/evo_lateral/logs/"
    },
    "data_reader": {
        "max_obj_num": 90,
        "resize_size": [
            192, // height
            320 // width
        ],
        "img_down_ratio": 1.0, //[height, width]
        "coors_down_ratio": [
            1.0,
            1.0
        ], //[height, width]
        "shuffle": true,
        "tasks": [
            {
                "preprocess": "obj_det",
                "read_img": true,
                "branches": [
                    "obj_center",
                    "tl",
                    "br"
                ],
                "category_path": "./config/face.txt",
                "train_folder": "/work/anders1234/WF/kps/train_kps",
                "test_folder": "/work/anders1234/WF/kps/test_kps"
            }
        ],
        "augments": {
            "color_chains": [
                "contrast",
                "saturat",
                "bright",
                "hue"
            ],
            "tensorpack_aug_chain": [], //"CropTransform","RandomPaste","JpegNoise","GaussianNoise","SaltPepperNoise"
            "do_flip": true
        },
        
        "img_root": "/work/anders1234/WF/imgs"
    },
    "models": {
        "model_name": "obj_det",
        "frozen_backbone": false,
        "multi_optimizer": false,
        "optimizer": "adam",
        "backbone": {
            "module_name": "hardnet",
            "optimizer": "adam",
            "num_vars": null,
            "variables": null,
            "lr": 1e-3
        },
        "neck": {
            "module_name": "sfpn",
            "optimizer": "adam",
            "num_vars": null,
            "variables": null,
            "lr": 1e-3
        },
        "head": {
            "module_name": "head",
            "optimizer": "adam",
            "num_vars": null,
            "variables": null,
            "lr": 1e-3,
            "pred_layer": {
                "heat_maps": [
                    {
                        "name": "obj_heat_map",
                        "out_dims": 1
                    }
                ],
                "size_maps": [
                    {
                        "name": "obj_size_maps",
                        "out_dims": 2
                    }
                ]
            }
        },
        "loss": { //weighted constant based on DETR paper
            "type": "center_od",
            "size_weight":1
        }
    }
}