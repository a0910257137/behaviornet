{
    "train_batch_size": 32,
    "test_batch_size": 32,
    "epochs":70,
    "learn_rate": 0.00025,
    "model_path": "/home2/user/anders/archive_model/pose_std",
    "summary": {
        "scalar_freq": 50,
        "hist_freq": 150,
        "img_freq": 250,
        "log_dir": "/home2/user/anders/archive_model/pose_std/logs/"
    },
    "data_reader": {
        "max_obj_num": 15,
        "resize_size": [
            192, // height
            320 // width
        ],
        "img_down_ratio": 1.0, //[height, width]
        "coors_down_ratio": [
            1.0,
            1.0
        ], //[height, width]
        "shuffle": true,
        "tasks": [{
            "preprocess": "obj_det",
            "read_img": true,
            "num_lnmks": 70,
            "branches": [
                "keypoint"
            ],
            "category_path": "./config/face.txt",
            "train_folder": "/home2/user/anders/300VW/tf_records/train",
            "test_folder": "/home2/user/anders/300VW/tf_records/test"
        }],
        "augments": {
            "color_chains": [
                "contrast",
                "saturat",
                "bright",
                "hue"
            ],
            "tensorpack_chains": ["WarpAffineTransform"], //"OverlayingMask", "CropTransform", "WarpAffineTransform","RandomPaste","JpegNoise","GaussianNoise","SaltPepperNoise"
            "album_chains": {
                "cutout": {
                    "max_holes": 15,
                    "min_holes": 5,
                    "max_height": 30,
                    "max_width": 30,
                    "min_height": 10,
                    "min_width": 10,
                    "fill_value": 0
                }
            },
            "do_flip": true
        },
        "3dmm":{
            "num_vertices":53215, 
            "model_path":"/home/anders/proj_py/3DHead/scripts/BFM/Out/BFM.mat",
            "pseudo_T_path":"/aidata/anders/objects/landmarks/3DDFA/pseudo_matrix/T.npy",
            "pseudo_exp_eq_left":"/aidata/anders/objects/landmarks/3DDFA/pseudo_matrix/exp_eq_left.npy",
            "pseudo_shp_eq_left": "/aidata/anders/objects/landmarks/3DDFA/pseudo_matrix/shp_eq_left.npy",
            "max_iter":5
        }
    },
    "models": {
        "model_name": "obj_det",
        "frozen_backbone": false,
        "multi_optimizer": false,
        "optimizer": "adam",
        "backbone": {
            "module_name": "hardnet",
            "optimizer": "adam",
            "num_vars": null,
            "variables": null,
            "lr": 1e-3
        },
        "neck": {
            "module_name": "fpn",
            "optimizer": "adam",
            "num_vars": null,
            "variables": null,
            "lr": 1e-3,
            "structure": {
                "inter_ch": 32,
                "skip_conv_ch": [
                    32,
                    32,
                    32,
                    0
                ]
            }
        },
        "head": {
            "module_name": "head",
            "optimizer": "adam",
            "num_vars": null,
            "variables": null,
            "lr": 1e-3,
            "pred_layer": {
                "heat_maps": [{
                    "name": "obj_heat_map",
                    "out_dims": 2
                }],
                "size_maps": [{
                    "name": "obj_size_maps",
                    "out_dims": 2
                }],
                "pose_maps": [{
                    "name": "obj_pose_map",
                    "out_dims": 3
                }]
            }
        },
        "loss": { //weighted constant based on DETR paper
            "type": "center_od",
            "size_weight": 1
        }
    }
}