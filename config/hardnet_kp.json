{
    "train_batch_size": 128,
    "test_batch_size": 128,
    "epochs": 150,
    "learn_rate": 0.0005,
    "model_path": "/aidata/anders/objects/landmarks/AFLW/archive_model/base",
    "summary": {
        "scalar_freq": 50,
        "hist_freq": 150,
        "img_freq": 250,
        "log_dir": "/aidata/anders/objects/landmarks/AFLW/archive_model/base/logs/"
    },
    "data_reader": {
        "max_obj_num": 15,
        "resize_size": [
            256, // height
            256 // width
        ],
        "img_down_ratio": 1.0, //[height, width]
        "coors_down_ratio": [
            1.0,
            1.0
        ], //[height, width]
        "shuffle": true,
        "video_folder":"/aidata/anders/objects/landmarks/demo_video/test/video_pool",
        "tasks": [
            {
                "preprocess": "keypoint",
                "num_lnmks":70,// landmark keypoints + 2 for tl and br
                "read_img": true,
                "branches":["keypoint"],
                "category_path": "./config/face.txt",
                
                "train_folder": "/home/anders/Downloads/CelebA/tf_records_68/train",
                "test_folder": "/home/anders/Downloads/CelebA/tf_records_68/test"
            }
        ],
        "augments": {
            "color_chains": [
                "contrast",
                "saturat",
                "bright",
                "hue"
            ],
            "tensorpack_chains": ["OverlayingMask",  "WarpAffineTransform"], //"CropTransform", "WarpAffineTransform","RandomPaste","JpegNoise","GaussianNoise","SaltPepperNoise"
            "album_chains": {
                "cutout": {
                    "max_holes": 15,
                    "min_holes": 5,
                    "max_height": 30,
                    "max_width": 30,
                    "min_height": 10,
                    "min_width": 10,
                    "fill_value": 0
                }
            },
            "do_flip": false
        }
    },
    "models": {
        "model_name": "obj_det",
        "frozen_backbone": false,
        "multi_optimizer": false,
        "optimizer": "adam",
        "backbone": {
            "module_name": "hardnet",
            "optimizer": "sgd",
            "num_vars": null,
            "variables": null,
            "lr": 1e-3
        },
        "neck": {
            "module_name": null,
            "optimizer": "sgd",
            "num_vars": null,
            "variables": null,
            "lr": 1e-3
        },
        "head": {
            "module_name": "head",
            "optimizer": "sgd",
            "num_vars": null,
            "variables": null,
            "lr": 1e-3,
            "d_model":512,
            "pred_layer": {
                "num_landmarks": 25
            }
        },
        "loss": { 
            "type": "landmarks"
        }
    }
}